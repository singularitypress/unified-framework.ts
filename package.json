{
  "name": "unified-framework.ts",
  "version": "1.0.0",
  "description": "A unifying GraphQL interface for different REST endpoints.",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build:db": "ts-node ./scripts",
    "start": "ts-node server",

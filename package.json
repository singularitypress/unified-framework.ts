  },
  "homepage": "https://github.com/singularitypress/unified-framework.ts#readme",
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.14",
    "@types/node": "^18.7.21",
    "nodemon": "^2.0.20",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.0",
    "typescript": "^4.8.3"
  },
  "dependencies": {
    "@graphql-tools/schema": "^9.0.4",
    "cors": "^2.8.5",
    "dotenv": "^16.0.2",
